{
  "manifest_version": 3,
  "name": "Simple New Tab Page",
  "description": "Show a random image/video and the time/date on your new tab page  ",
  "version": "0.0.0.5",
  "permissions" : [
      "tabs",
      "sessions",
      "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "newpage.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16":  "/images/icon-16.png",
      "32":  "/images/icon-32.png",
      "48":  "/images/icon-48.png",
      "128": "/images/icon-128.png",
      "512": "/images/icon-512.png"
    }
  },
  "icons": {
    "16":  "/images/icon-16.png",
    "32":  "/images/icon-32.png",
    "48":  "/images/icon-48.png",
    "128": "/images/icon-128.png",
    "512": "/images/icon-512.png"
  }
 }